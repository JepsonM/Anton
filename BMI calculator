namespace BMI_calculator
{
    class Program
    {
        static void Main(string[] args)
        {
            int weightInPounds;
            Console.Write("WeightinKillogram ");
            weightInPounds = Convert.ToInt32(Console.ReadLine());
            int heightInInches;
            Console.Write("HeightinMeter ");
            heightInInches = Convert.ToInt32(Console.ReadLine());
            double bmi = (weightInPounds) / (heightInInches * heightInInches); // double required since the result of the calculation is not integer.

            if (bmi < 18.5)
                Console.WriteLine("Underweight");
            if (18.5 < bmi & bmi < 24.9)
                Console.WriteLine("Normal");
            if (25 < bmi & bmi < 29.9)
                Console.WriteLine("Overweight");
            if (bmi == 30 | bmi > 30) // bmi = 30 "or" greater than 30
                Console.WriteLine("Obese");


        }
    }
}
